<configuration>
  <connectionStrings>
    <add name="PharmaCareDB" connectionString="Data Source=(LocalDB)\MSSQLLocalDB;AttachDbFilename=|DataDirectory|\PharmaCareDB.mdf;Integrated Security=True;Connect Timeout=30"
      providerName="System.Data.SqlClient" />
  </connectionStrings>
  <system.web>
    <compilation debug="true" targetFramework="4.6.1"/>
    <httpRuntime targetFramework="4.6.1"/>
  </system.web>
  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="BasicHttpBinding_ICocktailService" />
      </basicHttpBinding>
    </bindings>
    <client>
      <endpoint address="http://web.tafe.mst/CocktailService/CocktailService.svc"
        binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ICocktailService"
        contract="CocktailServiceReference.ICocktailService" name="BasicHttpBinding_ICocktailService" />
    </client>
  </system.serviceModel>
</configuration>